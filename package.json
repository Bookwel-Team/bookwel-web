{
  "name": "bookwel",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "prettier:check": "prettier --check \"src/**/*.{ts,tsx}\" \"./**/*.{json,yml,yaml}\"",
    "prettier:write": "prettier --write \"src/**/*.{ts,tsx}\" \"./**/*.{json,yml,yaml}\"",
    "cy:open": "cypress open",
    "cy:open-component": "cypress open --component",
    "test": "cypress run --component --spec './**/!(*.it).cy.tsx'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.4",
    "autoprefixer": "^10.4.17",
    "axios": "^1.6.7",
    "firebase": "^10.8.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.50.1",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.22.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.11.16",
    "@types/react": "^18.2.43",
    "@types/react-dom": "^18.2.17",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "@vitejs/plugin-react": "^4.2.1",
    "cypress": "^13.6.4",
    "daisyui": "^4.6.2",
    "eslint": "^8.55.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "postcss": "^8.4.35",
    "prettier": "^3.2.5",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.2.2",
    "vite": "^5.0.8"
  }
